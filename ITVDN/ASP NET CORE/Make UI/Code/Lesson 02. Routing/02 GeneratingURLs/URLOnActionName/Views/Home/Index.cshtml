
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Home Controller. Index View</title>
</head>
<body>
    @{
        int counter = 1;

        foreach (var item in Model)
        {
            <p>
                <a href="@item">Link @counter</a>
            </p>
            counter++;
        }
    }
</body>
</html>
